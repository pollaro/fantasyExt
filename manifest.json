{
	"name": "Fantasy Sports Stat Checker for Yahoo",
	"version": "1.0",
	"description": "Checks live stats for teams",
	"options_page": "html/options.html",
	"background": {
		"scripts": ["jquery-3.3.1.min.js", "js/background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_popup": "html/popup.html"
	},
	"permissions": [
		"https://api.login.yahoo.com/oauth*",
		"https://fantasysports.yahooapis.com/fantasy/v2*",
		"identity",
		"storage"
	],
	"manifest_version": 2
}
